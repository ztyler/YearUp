import static java.lang.System.out;

public class Fabric {

	public static void main(String[] args) {
		
		Atom element = new Atom(1, 0, 2);
		
		out.println("Name\tSymbol\tIsotope\tIon\tCharge");
		out.printf("%s\t%s\t%b\t%b\t%d",
				element.name,
				element.symbol,
				element.isIsotope,
				element.isIon,
				element.charge);
		
	}

}
