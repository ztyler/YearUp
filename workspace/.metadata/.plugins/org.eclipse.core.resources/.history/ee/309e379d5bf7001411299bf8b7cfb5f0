public class Atom {
	int protons, neutrons, electrons, charge;
	String name, symbol;
	boolean inonized, isotope;
	
	Atom(int p, int n, int e) {
		protons = p;
		neutrons = n;
		electrons = e;
		SetID();
	}
	
	private void SetID() {
		if (protons == 1) {
			name = "hydrogen";
			symbol = "H";
		}
	}
}
