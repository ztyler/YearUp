
public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Person Zach = new Person("Zach", "555-9050", "1686 Some Rd, Atlanta, GA", 22);
		
		System.out.printf("Name:%10s\n"
				+ "Phone:%10s\n"
				+ "Address:%10s\n"
				+ "Age:%12d\n",
				Zach.getName(),
				Zach.getPhoneNum(),
				Zach.getAddress(),
				Zach.getAge());
	}

}
