
public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Person Zach = new Person("Zach", "555-9050", "1686 Some Rd, Atlanta, GA", 22);
		
		System.out.printf("Name:\n\t%s\n"
				+ "Phone:\n\t%s\n"
				+ "Address:\n\t%s\n"
				+ "Age:\n\t%d\n",
				Zach.getName(),
				Zach.getPhoneNum(),
				Zach.getAddress(),
				Zach.getAge());
	}

}
