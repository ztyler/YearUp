
public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Person Zach = new Person("Zach", "555-9050", "1686 Some Rd, Atlanta, GA", 22);
		
		System.out.printf("NAME:\n%s\n"
				+ "PHONE:\n%s\n"
				+ "ADDRESS:\n%s\n"
				+ "AGE:\n%d\n",
				Zach.getName(),
				Zach.getPhoneNum(),
				Zach.getAddress(),
				Zach.getAge());
	}

}
